#include<stdio.h>
#include<stdlib.h>
int main(){
  int coin,i,Heads=0,Tails=0;
  printf("Tssing a coin...짜n");
  for(i=0;i<3;i++){
    coin=rand()%2;
    if(coin==0){
      printf("Round %d: Heads짜n",i+1);
      Heads++;
    }
    else{
      printf("Round %d: Tails짜n",i+1);
      Tails++;
    }
  }
  printf("Heads: %d, Tails: %d짜n",Heads,Tails);
}
